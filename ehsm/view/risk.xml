<mvc:View controllerName="ehsm.controller.riskasses"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
       <content>
     
     <VBox id="_IDGenVBox1"  alignItems = "Center" alignContent = "Center"  fitContainer = "true">
     <items>
      <Table id="RiskAsses" items="{/results}">
       <columns>
        <Column id="_IDGenColumn1" class="th">
         <Text id="_IDGenText1" text="Record Number"></Text>
        </Column>
        <Column id="_IDGenColumn2">
         <Text id="_IDGenText2" text="Created By"></Text>
        </Column>
        <Column id="_IDGenColumn3">
         <Text id="_IDGenText3" text="Updated By"></Text>
        </Column>
        <Column id="_IDGenColumn4">
         <Text id="_IDGenText4" text="Updated Date"></Text>
        </Column>
        <Column id="_IDGenColumn5">
         <Text id="_IDGenText5" text="Source data"></Text>
        </Column>
        <Column id="_IDGenColumn6">
         <Text id="_IDGenText6" text="Provider data"></Text>
        </Column>
        <Column id="_IDGenColumn7">
         <Text id="_IDGenText7" text="Work Area"></Text>
        </Column>
        <Column id="_IDGenColumn8">
         <Text id="_IDGenText19" text="Incident"></Text>
        </Column>
        <Column id="_IDGenColumn9">
         <Text id="_IDGenText20" text="Risk Assesment Number"></Text>
        </Column>
        <Column id="_IDGenColumn10">
         <Text id="_IDGenText21" text="Result"></Text>
        </Column>
       </columns>
       <items>
        <ColumnListItem id="_IDGenColumnListItem1">
         <cells>
          <Text id="_IDGenText8" text="{Recn}"></Text>
          <Text id="_IDGenText10" text="{Crnam}"></Text>
          <Text id="_IDGenText11" text="{Updnam}"></Text>
          <Text id="_IDGenText12" text="{Upddat}"></Text>
          <Text id="_IDGenText13" text="{Srsid}"></Text>
          <Text id="_IDGenText14" text="{Ownid}"></Text>
          <Text id="_IDGenText15" text="{Recntwah}"></Text>
          <Text id="_IDGenText16" text="{Rem}"></Text>
          <Text id="_IDGenText17" text="{Riskassetnum}"></Text>
          <Text id="_IDGenText18" text="{Rainvresult}"></Text>
         </cells>
        </ColumnListItem>
       </items>
      </Table>
     </items>
     </VBox>

    </content>
    <footer>
        <Bar id="_IDGenBar1">
        	<contentLeft>
        		<Button id="_IDGenButton1" text="Back" type="Accept" press="back"/>
        	</contentLeft>
            <contentRight>
                <Button id="_IDGenButton2" text="Logout" type="Accept" press="logout"/>
            </contentRight>
        </Bar>
    </footer>
    </Page>
</mvc:View>
