<mvc:View controllerName="quality.controller.usagedec"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="Usage Record">
    <headerContent>
        <Bar id="_IDGenBar2">
            <contentMiddle>
                <Text id="_IDGenText25" text="Usage Record"></Text>
            </contentMiddle>
        </Bar>
    </headerContent>
       <content>
     
     <VBox id="_IDGenVBox1"  alignItems = "Center" alignContent = "Center"  fitContainer = "true">
     <items>
      <Table id="Usage_dec" items="{/results}">
       <columns>
        <Column id="_IDGenColumn1" class="th">
         <Text id="_IDGenText1" text="Inspection plot"></Text>
        </Column>
        <Column id="_IDGenColumn2">
         <Text id="_IDGenText2" text="Plant"></Text>
        </Column>
        <Column id="_IDGenColumn3">
         <Text id="_IDGenText3" text="Vendor"></Text>
        </Column>
        <Column id="_IDGenColumn4">
         <Text id="_IDGenText4" text="Material"></Text>
        </Column>
        <Column id="_IDGenColumn5">
         <Text id="_IDGenText5" text="Po Number"></Text>
        </Column>
        <Column id="_IDGenColumn6">
         <Text id="_IDGenText6" text="Po Item"></Text>
        </Column>
        <Column id="_IDGenColumn7">
         <Text id="_IDGenText7" text="Material Doc"></Text>
        </Column>
        <Column id="_IDGenColumn17">
         <Text id="_IDGenText17" text="Matetrial Doc Item"></Text>
        </Column>
        <Column id="_IDGenColumn18">
         <Text id="_IDGenText18" text="Document Year"></Text>
        </Column>
       </columns>
       <items>
        <ColumnListItem id="_IDGenColumnListItem1">
         <cells>
          <Text id="_IDGenText8" text="{Insplot}"></Text>
          <Text id="_IDGenText9" text="{Plant}"></Text>
          <Text id="_IDGenText10" text="{Vendor}"></Text>
          <Text id="_IDGenText11" text="{Material}"></Text>
          <Text id="_IDGenText12" text="{PoNumber}"></Text>
          <Text id="_IDGenText13" text="{PoItem}"></Text>
          <Text id="_IDGenText14" text="{MatDoc}"></Text>
          <Text id="_IDGenText15" text="{MatdocItm}"></Text>
          <Text id="_IDGenText16" text="{DocYear}"></Text>
         </cells>
        </ColumnListItem>
       </items>
      </Table>
     </items>
     </VBox>

    </content>
    <footer>
        <Bar id="_IDGenBar1">
        	<contentLeft>
        		<Button id="_IDGenButton1" text="Back" type="Accept" press="back"/>
        	</contentLeft>
            <contentRight>
                <Button id="_IDGenButton2" text="Logout" type="Accept" press="logout"/>
            </contentRight>
        </Bar>
    </footer>
    </Page>
</mvc:View>
