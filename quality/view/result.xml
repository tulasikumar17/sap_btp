<mvc:View controllerName="quality.controller.result"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="Result Record">
    <headerContent>
        <Bar id="_IDGenBar2">
            <contentMiddle>
                <Text id="_IDGenText15" text="Result Record"></Text>
            </contentMiddle>
        </Bar>
    </headerContent>
       <content>
     
     <VBox id="_IDGenVBox1"  alignItems = "Center" alignContent = "Center"  fitContainer = "true">
     <items>
      <Table id="Result" items="{/results}">
       <columns>
        <Column id="_IDGenColumn1" class="th">
         <Text id="_IDGenText1" text="Inspection lot Num"></Text>
        </Column>
        <Column id="_IDGenColumn2">
         <Text id="_IDGenText2" text="Current Node Num"></Text>
        </Column>
        <Column id="_IDGenColumn3">
         <Text id="_IDGenText3" text="Inpection charc Num"></Text>
        </Column>
        <Column id="_IDGenColumn4">
         <Text id="_IDGenText4" text="Result Rec Status"></Text>
        </Column>
        <Column id="_IDGenColumn5">
         <Text id="_IDGenText5" text="User Created DR"></Text>
        </Column>
        <Column id="_IDGenColumn6">
         <Text id="_IDGenText6" text="Created Date"></Text>
        </Column>
        <Column id="_IDGenColumn7">
         <Text id="_IDGenText7" text="Changed Date "></Text>
        </Column>
        <Column id="_IDGenColumn8">
         <Text id="_IDGenText16" text="Num of insp Sample units"></Text>
        </Column>
        <Column id="_IDGenColumn9">
         <Text id="_IDGenText17" text="Result Confirmation"></Text>
        </Column>
       </columns>
       <items>
        <ColumnListItem id="_IDGenColumnListItem1">
         <cells>
          <Text id="_IDGenText8" text="{Prueflos}"></Text>
          <Text id="_IDGenText9" text="{Vorglfnr}"></Text>
          <Text id="_IDGenText10" text="{Merknr}"></Text>
          <Text id="_IDGenText11" text="{Satzstatus}"></Text>
          <Text id="_IDGenText12" text="{Ersteller}"></Text>
          <Text id="_IDGenText13" text="{Erstelldat}"></Text>
          <Text id="_IDGenText20" text="{Changeddatetime}"></Text>
          <Text id="_IDGenText21" text="{Anzwertg}"></Text>
          <Text id="_IDGenText24" text="{Sendeflag}"></Text>
         </cells>
        </ColumnListItem>
       </items>
      </Table>
     </items>
     </VBox>

    </content>
    <footer>
        <Bar id="_IDGenBar1">
        	<contentLeft>
        		<Button id="_IDGenButton1" text="Back" type="Accept" press="back"/>
        	</contentLeft>
            <contentRight>
                <Button id="_IDGenButton2" text="Logout" type="Accept" press="logout"/>
            </contentRight>
        </Bar>
    </footer>
    </Page>
</mvc:View>
